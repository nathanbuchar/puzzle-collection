# Google Sheets Configuration
REACT_APP_SPREADSHEET_ID=your_spreadsheet_id_here

# Google Service Account Credentials (JSON string)
# Get this from Google Cloud Console after creating a service account
GOOGLE_SERVICE_ACCOUNT_KEY={"type":"service_account","project_id":"..."}

# Dropbox Photo Base URL
# This should be the direct download link to your photos folder
REACT_APP_DROPBOX_BASE_URL=https://dl.dropboxusercontent.com/scl/fi/your-folder-path/photos
